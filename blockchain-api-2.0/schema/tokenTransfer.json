{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "tokenTransfer.schema.json",
    "title": "tokenTransfer",
    "description": "batch json schema",
    "type": "object",
    "properties": { 
      "from":{
          "description": "from address",
          "type":"string"
      },         
      "to": {
        "description": "Name of the startup which will receive the transfer",
        "type": "string"
      },
      "tokenId":{
        "description": "metadata of a toiken",
        "type": "string"
      }
    },
    "required": [ "to","tokenId","tokenURI"],
    "additionalProperties": false
  }